{
  "manifest_version": 3,
  "name": "Download Monitor",
  "version": "1.0",
  "description": "Monitor and track user downloads with timestamps",
  "permissions": [
    "downloads",
    "downloads.shelf",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Download Monitor",
    "default_icon": "icons/icon.png"
     
}
}